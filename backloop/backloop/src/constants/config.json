{
  "name": "Network",
  "version": "1.1",
  "channels": {
    "mychannel": {
      "orderers": [
        "orderer.example.com"
      ],
      "peers": [
        "peer0.org1.example.com",
        "peer0.org2.example.com"
      ]
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "peer0.org1.example.com"
      ]
    },
    "Org2": {
      "mspid": "Org2MSP",
      "peers": [
        "peer0.org2.example.com"
      ]
    }
  },
  "orderers": {
    "orderer.example.com": {
      "url": "grpcs://localhost:7050",
      "grpcOptions": {
        "ssl-target-name-override": "orderer.example.com"
      },
      "tlsCACerts": {
        "path": "../connections/fabric-dev/ordererOrganizations/example.com/orderers/orderer.example.com/tls/ca.crt"
      }
    }
  },
  "peers": {
    "peer0.org1.example.com": {
      "url": "grpcs://localhost:7051",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org1.example.com"
      },
      "tlsCACerts": {
        "path": "../connections/fabric-dev/peerOrganizations/org1.example.com/tlsca/tlsca.org1.example.com-cert.pem"
      }
    },
    "peer0.org2.example.com": {
      "url": "grpcs://localhost:9051",
      "grpcOptions": {
        "ssl-target-name-override": "peer0.org2.example.com"
      },
      "tlsCACerts": {
        "path": "../connections/fabric-dev/peerOrganizations/org2.example.com/tlsca/tlsca.org2.example.com-cert.pem"
      }
    }
  },
  "certificateAuthorities": {
    "ca-org1": {
      "url": "https://localhost:7054",
      "caName": "ca-org1",
      "tlsCACerts": {
        "path": "../connections/fabric-dev/fabric-ca/org1/ca-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    },
    "ca-org2": {
      "url": "https://localhost:8054",
      "caName": "ca-org2",
      "tlsCACerts": {
        "path": "../connections/fabric-dev/fabric-ca/org2/ca-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    },
    "ca-orderer": {
      "url": "https://localhost:9054",
      "caName": "ca-orderer",
      "tlsCACerts": {
        "path": "../connections/fabric-dev/fabric-ca/ordererOrg/ca-cert.pem"
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
